public class Compte {
	private int numero;
	private double solde;

	Compte(int numC) {
		this.numero = numC;
	}

	int getNumero() {
		return this.numero;
	}

	public double getSolde() {
		return solde;
	}

	public void setSolde(double solde) {
		this.solde = solde;
	}

	void retrait(double m) {
		solde = solde - m;
	}

	void depot(double m) {
		solde = solde + m;
	}
	
	@Override
	public String toString()
	{
		String s= "num compte: "+ this.numero + " solde: "+this.solde;
		return s;
	}
}
